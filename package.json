{
  "name": "eMia",
  "version": "0.1.1",
  "description": "eMia React Native. Server side",
  "main": "index.js",
  "author": "Sergey Krotkih <sergey.krotkih@gmail.com>",
  "license": "ISC",
  "scripts": {
    "start": "node ./bin/www",
    "startserver": "cross-env NODE_ENV=production node index.js",
    "dev": "cross-env NODE_ENV=development concurrently \"npm run server\" \"npm run client\"",
    "server": "nodemon index.js",
    "client": "npm run start --prefix client",
    "client:install": "npm install --prefix client",
    "client:build": "npm run build --prefix client"
  },
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.19.0",
    "cloudinary": "^1.11.0",
    "cookie-parser": "~1.4.4",
    "cors": "^2.8.4",
    "debug": "~2.6.9",
    "dotenv": "^6.2.0",
    "express": "^4.17.1",
    "express-validator": "^6.3.0",
    "http-errors": "~1.6.3",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^5.8.1",
    "morgan": "~1.9.1",
    "pug": "2.0.0-beta11",
    "shortid": "^2.2.15"
  },
  "devDependencies": {
    "concurrently": "^5.0.2",
    "cross-env": "^6.0.3",
    "nodemon": "^2.0.2"
  }
}
